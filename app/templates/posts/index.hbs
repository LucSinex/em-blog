{{#paper-content classNames="md-whiteframe-z1"}}
	{{#paper-list}}
		{{#each model as |post|}}
			{{#paper-item class="md-3-line"}}
				<div class="md-list-item-text">
					<h3>{{#link-to 'posts.show' post}}{{post.title}}{{/link-to}}</h3>
					<h4>
						{{post.body}}
					</h4>
				</div>
			{{/paper-item}}
		{{/each}}
	{{/paper-list}}
{{/paper-content}}



{{outlet}}